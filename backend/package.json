{
  "name": "agence-immobiliere-backend",
  "version": "1.0.1",
  "description": "Backend API pour l'agence immobilière",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "test": "cross-env NODE_ENV=test dotenv -e .env.test -- jest --coverage --verbose",
    "test:watch": "jest --watch",
    "test:ci": "jest --ci --coverage --maxWorkers=2",
    "lint": "eslint src tests",
    "lint:fix": "eslint src tests --fix",
    "db:migrate": "node scripts/migrate.js up",
    "db:migrate:down": "node scripts/migrate.js down",
    "db:migrate:status": "node scripts/migrate.js status",
    "db:seed": "node scripts/seed.js seed",
    "db:seed:clear": "node scripts/seed.js clear",
    "db:seed:reseed": "node scripts/seed.js reseed",
    "db:setup": "npm run db:migrate && npm run db:seed"
  },
  "keywords": [
    "immobilier",
    "api",
    "nodejs",
    "express"
  ],
  "author": "Votre équipe",
  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "connect-mongo": "^5.1.0",
    "cors": "^2.8.5",
    "csv-parser": "^3.2.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-session": "^1.18.2",
    "express-validator": "^7.0.1",
    "fast-csv": "^5.0.5",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.0.3",
    "morgan": "^1.10.0",
    "multer": "^2.0.2",
    "nodemailer": "^7.0.10",
    "openai": "^6.9.1",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "prom-client": "^14.0.0",
    "qrcode": "^1.5.4",
    "socket.io": "^4.8.1",
    "stripe": "^17.5.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "winston": "^3.9.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.11",
    "cross-env": "^7.0.3",
    "dotenv-cli": "^7.0.0",
    "eslint": "^8.56.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.29.1",
    "jest": "^29.7.0",
    "mongodb-memory-server": "^10.3.0",
    "nodemon": "^3.0.2",
    "supertest": "^6.3.3"
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  }
}
