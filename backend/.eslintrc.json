{
  "env": {
    "node": true,
    "es2021": true,
    "jest": true
  },
  "extends": [
    "airbnb-base"
  ],
  "parserOptions": {
    "ecmaVersion": 12
  },
  "rules": {
    "no-console": "off",
    "comma-dangle": ["error", "never"],
    "linebreak-style": "off",
    "no-underscore-dangle": "off",
    "consistent-return": "off",
    "no-unused-vars": ["warn", { "argsIgnorePattern": "^_" }]
  }
}
