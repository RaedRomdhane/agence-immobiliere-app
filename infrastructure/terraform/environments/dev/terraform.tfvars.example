# Exemple de fichier de variables pour l'environnement DEV
# Copier ce fichier en terraform.tfvars et remplir les valeurs

# Region AWS
region = "eu-west-3"

# Configuration VPC
vpc_cidr = "10.0.0.0/16"
public_subnet_cidrs = ["10.0.1.0/24", "10.0.2.0/24"]
private_subnet_cidrs = ["10.0.10.0/24", "10.0.20.0/24"]
availability_zones = ["eu-west-3a", "eu-west-3b"]

# Configuration Base de données
db_username = "admin"
db_port = 27017

# IPs autorisées pour SSH (OBLIGATOIRE)
# Remplacer par vos IPs publiques
ssh_allowed_ips = [
  # "203.0.113.0/32",  # Exemple: Votre IP publique
  # "198.51.100.0/24", # Exemple: Votre réseau d'entreprise
]
