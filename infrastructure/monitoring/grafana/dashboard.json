{
  "annotations": {"list": []},
  "panels": [
    {
      "type": "graph",
      "title": "CPU (process)",
      "targets": [
        {"expr": "process_cpu_seconds_total", "legendFormat": "cpu"}
      ]
    },
    {
      "type": "graph",
      "title": "Memory (RSS)",
      "targets": [
        {"expr": "process_resident_memory_bytes", "legendFormat": "rss"}
      ]
    },
    {
      "type": "graph",
      "title": "HTTP p95 latency (s)",
      "targets": [
        {"expr": "histogram_quantile(0.95, sum(rate(http_request_duration_seconds_bucket[5m])) by (le))", "legendFormat": "p95"}
      ]
    },
    {
      "type": "graph",
      "title": "5xx errors (rate)",
      "targets": [
        {"expr": "sum(rate(http_requests_total{code=~\"5..\"}[5m]))", "legendFormat": "5xx/s"}
      ]
    }
  ],
  "schemaVersion": 16,
  "title": "Agence - Basic Monitoring"
}
